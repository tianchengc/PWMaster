<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>json-form</servlet-name>
        <jsp-file>/jsp/json-form.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>json-form</servlet-name>
        <url-pattern>/json-form</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>regular-form</servlet-name>
        <jsp-file>/jsp/regular-form.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>regular-form</servlet-name>
        <url-pattern>/regular-form</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
